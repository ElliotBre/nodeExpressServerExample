{
    "name": "pydb",
    "description": "template package for my node applications",
    "type": "module",
    "dependencies": {
        "eslint-plugin-prettier": "5.0.0",
        "eslint-plugin-promise": "6.1.1",
        "express": "4.18.2",
        "node": "21.6.1",
        "path": "0.12.7",
        "url": "0.11.3",
        "ws": "8.16.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "7.22.15",
        "@babel/preset-env": "7.22.20",
        "eslint": "8.56.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "9.0.0",
        "eslint-plugin-import": "2.29.1",
        "husky": "9.0.10",
        "lint-staged": "15.2.2",
        "prettier": "3.2.5"
    },
    "versions": "1.0.0",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "npx prettier --write ./public ./server",
        "lint": "eslint public server --ext .js",
        "lint:fix": "eslint public server --ext .js --fix",
        "start": "node server.js",
        "prepare": "husky install"
    },
    "engines": {
        "node": "20.7.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ElliotBre/PyDB-.git"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 8,
            "sourceType": "module"
        },
        "env": {
            "node": true,
            "es6": true
        },
        "rules": {
            "no-console": 0,
            "no-unused-vars": 1
        }
    },
    "husky": {
        "hooks": {
          "pre-commit": "lint-staged"
        }
      },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC"
}

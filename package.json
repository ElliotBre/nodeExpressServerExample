{
    "name": "pydb",
    "description": "template package for my node applications",
    "type": "module",
    "dependencies": {
        "node": "21.6.1",
        "express": "4.18.2",
        "path": "0.12.7",
        "url": "0.11.3",
        "ws": "8.16.0",
        "eslint":"8.56.0"
    },
    "devDependencies": {
      "husky":"9.0.10",
      "prettier":"3.2.5",
      "lint-staged":"15.2.2"
    },
    "versions": "1.0.0",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "npx prettier --write ./src",
        "lint": "eslint src --ext .js",
        "lint:fix": "eslint src --ext .js --fix",
        "start": "node server.js"
    },
    "engines": {
      "node": "20.7.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ElliotBre/PyDB-.git"
    },
    "prettier": {
        "trailingComma": "es5",
        "printWidth": 80,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "bracketSpacing": true,
        "endOfLine": "auto"
    },
    "eslintConfig": {
      "extends": "eslint:recommended",
      "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
      },
      "env": {
        "node": true,
        "es6": true
      },
      "rules": {
        "no-console": 0,
        "no-unused-vars": 1
      }
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "src/**/*.js" : [
        "eslint --fix",
        "prettier --write"
        ]
      },
    "keywords": [],
    "author": "",
    "license": "ISC"
}

{
    "name": "pydb",
    "description": "template package for my node applications",
    "type": "module",
    "dependencies": {
        "node": "21.6.1",
        "express": "4.18.2",
        "path": "0.12.7",
        "url": "0.11.3",
        "ws": "8.16.0",
        "prettier": "3.2.5",
        "husky": "9.0.10",
        "lint-staged": "15.2.2",
        "eslint":"8.56.0"
    },
    "versions": "1.0.0",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ElliotBre/PyDB-.git"
    },
    "prettier": {
        "trailingComma": "es5",
        "printWidth": 80,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "bracketSpacing": true,
        "endOfLine": "auto"
    },
    "eslint": {
      "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.{js,jsx}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ],
      "*.{html,css,less,ejs}": [
        "prettier --write",
        "git add"
      ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC"
}
